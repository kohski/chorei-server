{
  "swagger": "2.0",
  "info": {
    "title": "Chorei API",
    "version": "v1"
  },
  "basePath": "/api/v1",
  "paths": {
    "/auth": {
      "post": {
        "summary": "Create User",
        "tags": [
          "Users"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "user",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "email": {
                  "type": "string"
                },
                "descriptiom": {
                  "type": "string"
                },
                "image": {
                  "type": "string"
                },
                "password": {
                  "type": "string"
                }
              },
              "required": [
                "title",
                "email",
                "password"
              ]
            }
          }
        ],
        "responses": {
          "201": {
            "description": "User Sign up",
            "examples": {
              "application/json": {
                "status": "success",
                "message": "User Sign Up",
                "data": null
              }
            }
          }
        }
      }
    }
  }
}